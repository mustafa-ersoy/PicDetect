# Ignore node_modules from the frontend (React)
/frontend/node_modules/

# Ignore build artifacts and compiled code from the frontend (React)
/frontend/build/
/frontend/dist/
/frontend/.next/
/frontend/.cache/
/frontend/.idea/
/frontend/.vscode/
/frontend/*.log

# Ignore virtual environment from the backend (Django)
/backend/venv/
/backend/__pycache__/

